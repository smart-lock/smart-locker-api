{
  "env": "prod",
  "service": {
    "port": "{{PORT}}"
  },
  "token": {
    "issuer": "smart-lock",
    "audience": "user",
    "jwtDuration": "30d",
    "publicKeyPath": "prod/pub.pem",
    "privateKeyPath": "prod/priv.pem",
    "bucketName": "smart-lock-secrets"
  },
  "mqtt": {
    "uri": "{{MQTT_URI}}"
  },
  "redis": {
    "uri": "{{REDIS_URI}}"
  },
  "prisma": {
    "uri": "{{PRISMA_URI}}"
  }
}